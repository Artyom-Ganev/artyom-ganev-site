{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -o production -d build -t -s **/* !node_modules",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "prettier": "prettier -c --write \"src/**/*.ts?\"",
    "lint": "eslint --fix \"src/**/*.ts?\""
  },
  "dependencies": {
    "axios": "0.19.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "12.12.31",
    "@types/react": "16.9.26",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/react-redux": "7.1.7",
    "@types/redux-actions": "2.6.1",
    "@types/reselect": "2.2.0",
    "@typescript-eslint/parser": "2.25.0",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.9",
    "node-sass": "4.13.1",
    "prettier": "2.0.2",
    "react-scripts": "3.4.1",
    "sass": "1.26.3",
    "typescript": "3.8.3"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^actions$": "<rootDir>/src/actions",
      "^assets$": "<rootDir>/src/assets",
      "^components$": "<rootDir>/src/components",
      "^components/Layout$": "<rootDir>/src/components/Layout",
      "^components/Navigation$": "<rootDir>/src/components/Navigation",
      "^components/Pages$": "<rootDir>/src/components/Pages",
      "^const$": "<rootDir>/src/const",
      "^container$": "<rootDir>/src/container",
      "^model$": "<rootDir>/src/model",
      "^reducers$": "<rootDir>/src/reducers",
      "^store$": "<rootDir>/src/store",
      "^utils$": "<rootDir>/src/utils"
    },
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jestFileTransformer.js"
    }
  }
}
